nnoremap d <C-d>
nnoremap u <C-u>
nnoremap J gT
nnoremap K gt
nnoremap x d
nnoremap X u
nnoremap w k
nnoremap s j

" proxy
nnoremap "\\u" :set!<Space>network.proxy.type=1<Return>
